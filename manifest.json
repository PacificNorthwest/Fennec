{
    "name": "Fennec",
    "version": "1.0",
    "description": "An extension for convenient images sharing with my little fennec",
    "manifest_version": 2,
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "background": {
        "persistant": false,
        "scripts": [
            "main.js"
        ]
    },
    "icons": {
        "128": "fennec.png"
    },
    "browser_action": {
        "default_icon": "fennec.png"
    },
    "content_security_policy": "script-src 'self' 'sha256-huDFnItbMeySwBf+O4hH8gHG4SrPX4ikBQeKyMKUeUM='; object-src 'self'"
}